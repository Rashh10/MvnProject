pipeline {
    agent any
    stages {
        stage('Cleaning Stage') {
            steps {
                bat "mvn clean"
            }
        }
        stage('Testing Stage') {
            steps {
                bat "mvn test"
            }
        }
        stage('Packaging Stage') {
            steps {
                bat "mvn package"
            }
        }
        stage('SonarQube Stage') {
            steps {
                bat "mvn sonar:sonar -Dsonar.host.url=http://localhost:9000 -Dsonar.credentials=login:e82caea4f0cce3240aa168d09be7f161871dba5a"
            }
        }
    }
}
